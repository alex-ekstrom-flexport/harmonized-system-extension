{
  "plugins": [
    "prettier",
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "no-console": [
      1
    ]
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:storybook/recommended",
    "plugin:storybook/recommended"
  ],
  "globals": {
    "document": true,
    "window": true,
    "chrome": true,
    "console": true
  }
}